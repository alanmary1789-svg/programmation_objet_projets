    class NoeudT["Noeud&lt;T&gt;"] {
        <<template>>
        -T info
        -Noeud* suivant
        +Noeud()
        +Noeud(T valeur)
        +getInfo() T
        +getSuivant() Noeud*
        +setInfo(T inf) void
        +setSuivant(Noeud* next) void
    }

    class ListeT["Liste&lt;T&gt;"] {
        <<template>>
        -Noeud* tete
        -int taille
        +Liste()
        +Liste(const Liste& l)
        +~Liste()
        +getTete() Noeud*
        +getTaille() int
        +insererDebut(T valeur) void
        +insererFin(T valeur) void
        +supprimer(T valeur) bool
        +vider() void
        +rechercher(T val) Noeud*
        +contient(T val) bool
    }

